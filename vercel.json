{
    "builds" : [{
        "src" : "GreenTech/wsgi.py",
        "use" : "@vercel/python",
        "config" : {
                "maxLambdaSize" : "15mb",
                "runtime": "python3.12"
                    }
    }],
    "routes" : [
        {
            "src" : "/(.*)",
            "dest" : "GreenTech/wsgi.py"
        }
    ]
}